{
  "problemMatcher": [
    {
      "owner": "tfausak",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^(.+):(\\d+):(\\d+): (Error|Warning|Suggestion): (.+)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 5
        },
        {
          "regexp": "^(\\S+)$",
          "message": 1
        }
      ]
    }
  ]
}
